@page
@model MascotaFeliz.App.Frontend.Pages.ListaDuenosModel
@{
}

<h1>ListaDuenos</h1>
<table class="" >
    @foreach (var dueno in Model.listaDuenos)
    {
        <tr>
            <td>@dueno.Nombres</td>
            <td>@dueno.Apellidos</td>
            <td>@dueno.Direccion</td>
            <td>@dueno.Telefono</td>
            <td>@dueno.Correo</td>
            <td>
                <a class="btn  btn-primary table-btn" asp-page=".\DetalleDuenos" asp-route-duenoId="@dueno.Id" >Detalle Dueno</a>
            </td>
            <td>
                <a class="btn  btn-primary table-btn" asp-page=".\EditarDuenos" asp-route-duenoId="@dueno.Id" >Editar  Dueno</a>
            </td>
            <td>
                <a class="btn  btn-primary table-btn" asp-page=".\DeleteDueno" asp-route-duenoId="@dueno.Id" >Eliminar  Dueno</a>
            </td>

        </tr>
    
    }
</table>
<a class="btn  btn-primary table-btn" asp-page=".\EditarDuenos" >Agregar Dueño Nuevo</a>